{
  "ConnectionStrings": {
    "AppDbConnection": "DataSource=|DataDirectory|app.db",
    "MetricDbConnection": "DataSource=|DataDirectory|metric.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Error",
      "Microsoft.EntityFrameworkCore.Database.Command": "Error"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information"
  }
}